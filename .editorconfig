# editorconfig.org
root = true

[*]
indent_style = tab
tab_width = 4
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.jade]
trim_trailing_whitespace = false
insert_final_newline = false

[*.js]
indent_brace_style = 1TBS
spaces_around_brackets = outside
curly_bracket_next_line = true
