// NAVIGATION - NAVBAR SIDEBAR
nav#wrapper-navigation.navbar.navbar-inverse.navbar-static-top(role="navigation")
	.container-fluid
		// NAVBAR HEADER
		.navbar-header
			button#navbar-toggle.navbar-toggle.collapsed(type="button", data-toggle="collapse", href="#container-navbar", data-target="#container-navbar", aria-expanded="false", aria-controls="container-navbar")
				span.sr-only TOGGLE ALL NAVIGATION MENUS 
				span
					i.fa.fa-bars
			button#sidebar-toggle.sidebar-toggle.navbar-toggle.hidden-xs(type="button", aria-controls="sidebar")
				span.sr-only TOGGLE SIDEBAR VISIBILITY
				span
					i.fa.fa-bars
		// NAVBAR BRAND TITLE
		a.navbar-brand.navbar-text.hidden-xs(href="./") dh.technical.solutions
		// NAVBAR LOGO
		a.navbar-logo(href="./")
			img(src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/153562/dhts-android-icon-128x128.png")
		block NAVBAR_NAVS_HEADER
			include navbar.navs.header.jade
		// NAVBAR COLLAPSE
		#container-navbar.navbar-collapse.collapse
			block NAVBAR_NAVS_LINKS
				include navbar.dropdowns.jade
			block NAVBAR_SIDEBAR
				include navbar.sidebar.jade
		