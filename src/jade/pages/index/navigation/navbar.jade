// NAVIGATION - NAVBAR SIDEBAR
nav.navbar.navbar-inverse.navbar-static-top(role="navigation")
	.container-fluid
		// NAVBAR HEADER
		.navbar-header
			button#navbar-toggle.navbar-toggle.collapsed(type="button", data-toggle="collapse", href="#wrapper-sidebar,#sidebar,#navbar", data-target="#wrapper-sidebar,#sidebar,#navbar", aria-expanded="false", aria-controls="navbar")
				span.sr-only TOGGLE ALL NAVIGATION MENUS 
				span
					i.fa.fa-bars
			button#sidebar-toggle.sidebar-toggle.navbar-toggle.hidden-xs(type="button", aria-controls="sidebar")
				span.sr-only TOGGLE SIDEBAR VISIBILITY
				span
					i.fa.fa-bars
		// NAVBAR BRAND TITLE
		a.navbar-brand.hidden-xs(href='#') dh.technical.solutions
		// NAVBAR LOGO
		a.navbar-logo(href='#')
			img(src="https://s3-us-west-2.amazonaws.com/s.cdpn.io/153562/dhts-android-icon-128x128.png")
		block NAVBAR_NAVS_HEADER
			include navbar.navs.header.jade
		// NAVBAR COLLAPSE
		#navbar.navbar-collapse.collapse
			ul.nav.navbar-nav.navbar-navs-links
				block NAVBAR_LINKS
					include navbar.dropdowns.jade
		